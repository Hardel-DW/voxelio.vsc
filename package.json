{
    "name": "voxel.vsc",
    "private": true,
    "type": "module",
    "packageManager": "pnpm@9.15.0",
    "scripts": {
        "vsce:package": "pnpm build && cd packages/extension && npx vsce package --no-dependencies",
        "build": "pnpm -r build",
        "dev": "pnpm -r --parallel dev",
        "lint": "pnpm -r lint",
        "biome:check": "pnpm -r biome:check",
        "biome:format": "pnpm -r biome:format",
        "biome:unsafefix": "pnpm -r biome:unsafefix"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.10",
        "@typescript/native-preview": "7.0.0-dev.20251208.1"
    }
}